### dev setup

pip3 install virtualenv
sudo easy_install virtualenv
virtualenv -p python3 ml_test
cd ml_test
source bin/activate
pip install notebook tmm numpy scipy matplotlib p_tqdm ipywidgets pandas tensorflow keras
python -m ipykernel install --user --name=ml_test
# vim /Users/username/Library/Jupyter/kernels/ml_test/kernel.json
jupyter-notebok


